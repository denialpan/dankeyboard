<Window x:Name="dankeyboard" x:Class="dankeyboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dankeyboard"
        mc:Ignorable="d"
        Title="dankeyboard" Height="600" Width="850" ResizeMode="NoResize" Background="White">
    <Grid>
        <TabControl>
            <TabItem Header="keyboard">
                <Grid Background="#FFE5E5E5">
                    <Rectangle x:Name="Key_Tilde" HorizontalAlignment="Left" Height="35" Margin="10,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35" Fill="Red"/>
                    <Rectangle x:Name="Key_Tilde_Copy" HorizontalAlignment="Left" Height="35" Margin="50,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy1" HorizontalAlignment="Left" Height="35" Margin="89,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy2" HorizontalAlignment="Left" Height="35" Margin="128,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy3" HorizontalAlignment="Left" Height="35" Margin="167,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy4" HorizontalAlignment="Left" Height="35" Margin="206,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy5" HorizontalAlignment="Left" Height="35" Margin="245,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy6" HorizontalAlignment="Left" Height="35" Margin="284,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy7" HorizontalAlignment="Left" Height="35" Margin="323,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy8" HorizontalAlignment="Left" Height="35" Margin="362,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy9" HorizontalAlignment="Left" Height="35" Margin="401,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy10" HorizontalAlignment="Left" Height="35" Margin="440,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy11" HorizontalAlignment="Left" Height="35" Margin="479,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy12" HorizontalAlignment="Left" Height="35" Margin="518,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy13" HorizontalAlignment="Left" Height="35" Margin="557,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy14" HorizontalAlignment="Left" Height="35" Margin="596,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy15" HorizontalAlignment="Left" Height="35" Margin="635,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy16" HorizontalAlignment="Left" Height="35" Margin="674,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy17" HorizontalAlignment="Left" Height="35" Margin="713,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <Rectangle x:Name="Key_Tilde_Copy18" HorizontalAlignment="Left" Height="35" Margin="752,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="35"/>
                    <TextBlock HorizontalAlignment="Left" Margin="12,12,0,0" TextWrapping="Wrap" Text="`" VerticalAlignment="Top" Width="31" Height="31" TextAlignment="Center" FontSize="20"/>
                    <TextBlock HorizontalAlignment="Left" Margin="52,12,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="31" Height="31" TextAlignment="Center" FontSize="20"/>
                    <TextBlock HorizontalAlignment="Left" Margin="91,12,0,0" TextWrapping="Wrap" Text="A" VerticalAlignment="Top" Width="31" Height="31" TextAlignment="Center" FontSize="20"/>
                    <TextBlock HorizontalAlignment="Left" Margin="130,12,0,0" TextWrapping="Wrap" Text="X" VerticalAlignment="Top" Width="31" Height="31" TextAlignment="Center" FontSize="20"/>
                </Grid>
            </TabItem>
            <TabItem Header="monitor">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
